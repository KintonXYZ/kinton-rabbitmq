[
  { rabbit, [
      { loopback_users, []},
      { auth_backends, [rabbit_auth_backend_http]}
    ]
  },
  { rabbitmq_mqtt, [
      { subscription_ttl, undefined},
      { tcp_listeners,    [1884]}
    ]
  },
  { rabbitmq_auth_backend_http, [
      { http_method,   post},
      { user_path,     "http://AUTH_BACKEND:3000/auth/user"},
      { vhost_path,    "http://AUTH_BACKEND:3000/auth/vhost"},
      { resource_path, "http://AUTH_BACKEND:3000/auth/resource"}
    ]
  }
].
